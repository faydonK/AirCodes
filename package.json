{
  "name": "aircodes",
  "version": "1.0.0",
  "description": "AirCodes is an open-source, community-focused platform designed for everyone. It encourages active participation and responsiveness through a unique coin system.",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "dev:server": "node src/server/index.js",
    "dev:web": "vite",
    "build": "vite build",
    "start": "node src/server/index.js",
    "start:bot": "node src/bot/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "pm2": "^6.0.8",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.5",
    "@types/passport": "^1.0.16",
    "@types/passport-discord": "^0.1.6",
    "concurrently": "^8.2.2",
    "eslint": "^9.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.3.0",
